setwd("~/Documents/R Data")
lab10.data<-read.csv("lab 10 sra 365 wc.csv", header = TRUE)

library(gmodels)

#predictor variable first, outcome second
CrossTable(lab10.data$num_people_v2, lab10.data$cost_controls_v2, fisher = TRUE,
           chisq = TRUE, expected = TRUE, 
           sresid = TRUE, format = "SPSS")

CrossTable(lab10.data$per_sensitive_v2, lab10.data$cost_controls_v2, fisher = TRUE,
           chisq = TRUE, expected = TRUE, 
           sresid = TRUE, format = "SPSS")

CrossTable(lab10.data$dys_detect_v2, lab10.data$cost_controls_v2, fisher = TRUE,
           chisq = TRUE, expected = TRUE, 
           sresid = TRUE, format = "SPSS")
