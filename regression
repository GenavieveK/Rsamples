setwd("~/Documents/R Data")
Album<-read.delim("Album Data 1.dat", header = TRUE) 
Album<-read.delim("Album Sales 1.dat", header = TRUE) 
Album<-lm(sales ~ adverts, data = album1, na.action = na.exclude)
Album
summary(Album)
Album<-read.delim("Album Sales 1.dat", header = TRUE) 
albumRegress<-lm(sales ~ adverts, data = Album, na.action = na.exclude)
summary(albumRegress)
predict(Album, data.frame(adverts = 200), interval = "prediction")
predict(Album, data.frame(adverts = 200), interval = "confidence")
predict(albumRegress, data.frame(adverts = 200), interval = "prediction")
predict(albumRegress, data.frame(adverts = 200), interval = "confidence")
predict(albumRegress, data.frame(adverts = 200), interval = "confidence", level = .80)

setwd("~/Documents/R Data")
lab5.data<-read.delim("lab 5 sra 365 wc.dat", header = TRUE)
negFin<-lm(dys_impact ~ per_sensitive, data = lab5.data, na.action = na.exclude)
summary(negFin)
moneySpent<-lm(cost_controls ~ per_sensitive, data = lab5.data, na.action = na.exclude)
summary(moneySpent)
numRecs<-lm(num_records ~ dys_impact, data = lab5.data, na.action = na.exclude)
summary(numRecs)
numRecs2<-lm(dys_impact ~ num_records, data = lab5.data, na.action = na.exclude)
summary(numRecs2)
numBreach<-lm(cost_controls ~ num_records, data = lab5.data, na.action = na.exclude)
summary(numBreach)

scatter<-ggplot(lab5.data, aes(num_records, cost_controls))
